{
  "name": "eva-orchestrator",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node src/orchestrator.ts",
    "orchestrate": "node --enable-source-maps dist/orchestrator.js",
    "start": "npm run dev",
    "update-dashboard": "node --enable-source-maps dist/agents/update-dashboard-agent.js"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.0",
    "js-yaml": "^4.1.0",
    "p-limit": "^6.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
